{
    "name": "supertype",
    "description": "A type system for classical inheritence, mix-ins and composition.",
    "version": "3.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/haven-life/supertype",
    "dependencies": {
        "@types/reflect-metadata": "^0.1.0",
        "reflect-metadata": "^0.1.12"
    },
    "devDependencies": {
        "@types/chai": "^3.4.34",
        "@types/mocha": "^2.2.39",
        "@types/node": "^7.0.5",
        "chai": "*",
        "eslint": "3.7.x",
        "istanbul": "0.4.5",
        "q": "1.x",
        "sinon": "1.15.x",
        "typescript": "^2.8.4",
        "mocha": "5.2.0",
        "ts-node": "*"
    },
    "scripts": {
        "lint": "eslint .",
        "debugTests": "mocha --inspect-brk test/animals.js",
        "debugTSTests": "mocha --inspect-brk test/typescript/*.js",
        "test:typescript": "mocha --compilers ts:ts-node/register,tsx:ts-node/register test/typescript/*",
        "clean": "rm -rf dist/ && rm -rf types/ && rm -rf node_modules/",
        "compile": "tsc",
        "cleanDist": "rm -rf dist/",
        "test": "mocha test/**/*.js dist/test/**/*.js",
        "test:config": "mocha test/config",
        "test:example": "mocha test/example"
    },
    "directories": {},
    "engines": {
        "node": ">=0.8.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/haven-life/supertype.git"
    },
    "bugs": {
        "url": "http://github.com/haven-life/supertype/issues"
    },
    "optionalDependencies": {}
}
